{
  "name": "docker-playground - python",
  "dockerComposeFile": "docker-compose.yml",  
  "service": "playground",
  "workspaceFolder": "/workspace",
  "remoteUser": "vscode",
  "customizations": {
    "vscode": {
      "extensions": [
        "EditorConfig.EditorConfig",
        "ms-python.python",
        "ms-python.flake8",
        "ms-python.autopep8",
        "njpwerner.autodocstring"
      ]
    }
  }
}
